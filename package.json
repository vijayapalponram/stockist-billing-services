    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-bunyan-logger": "^1.3.2",
    "helmet": "^3.6.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.4",
    "node-rx-http": "^1.0.1",
    "prom-client": "^9.1.1",
